    <groupId>org.openjweb</groupId>
    <artifactId>openjweb-starter</artifactId>
    <packaging>pom</packaging>
    <version>0.0.1-SNAPSHOT</version>
    <name>openjweb-starter</name>
    <description>openjweb-starter</description>
    <url/>
    <licenses>
